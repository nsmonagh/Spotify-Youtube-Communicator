{
  "author":"Noah Monaghan",
  "content_scripts":[
    {
      "js":[
        "js/jquery.js",
        "js/waitForKeyElements.js",
        "js/spotify-web-api.js",
        "js/script.js"
      ],
      "matches":[
        "*://www.youtube.com/watch?*v=*"
      ],
      "run_at":"document_end"
    }
  ],
  "description":"Open up featured songs from YouTube videos onto Spotify.",
  "icons":{
    "16":"images/image16.png",
    "32":"images/image32.png",
    "48":"images/image48.png",
    "128":"images/image128.png"
  },
  "manifest_version":2,
  "name":"Songs on YouTube to Spotify",
  "version":"1.0",
  "web_accessible_resources":[
    "images/spotify32.png",
	"script.py"
  ]
}